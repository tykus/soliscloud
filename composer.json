{
    "name": "tykus/soliscloud",
    "description": "API Client and SDK library for the SolisCloud API",
    "keywords": [
        "tykus",
        "soliscloud-client",
        "soliscloud-sdk"
    ],
    "homepage": "https://github.com/tykus/soliscloud",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Brian O'Sullivan",
            "email": "brian@tykus.ie",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/support": "^10.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Tykus\\SolisCloud\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tykus\\SolisCloud\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Tykus\\SolisCloud\\SolisCloudServiceProvider"
            ],
            "aliases": {
                "SolisCloud": "Tykus\\SolisCloud\\SolisCloudFacade"
            }
        }
    }
}
